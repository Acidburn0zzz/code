<!-- 
     $Id$
     Freddie Akeroyd <F.A.Akeroyd@rl.ac.uk>
     Example of calling dynamic OpenGENIE retriever "libnxgenie.so"
     Note that you need to escape double quote (") characters
     using XML escape sequences e.g.

        GET("TITL") needs to be written as  GET(&quot;TITL&quot;)
-->
<NXroot>
<entry1 type="NXentry" NXS:source="hrp00273.raw" NXS:mime_type="dynamic/libnxgenie.so">
<title  NXS:location="GET(&quot;TITL&quot;)" />
<good_frames NXS:location="(GET(&quot;IRPB&quot;))[10]" />
<raw_frames NXS:location="(GET(&quot;IRPB&quot;))[11]" />
<run_number NXS:location="GET(&quot;RUN&quot;)" />
<program_name type="NX_CHAR">ICP1SEP89</program_name>
<run_cycle type="NX_CHAR">05/3</run_cycle>
<experiment_identifier NXS:location="&quot;RB&quot;+AS_STRING((GET(&quot;IRPB&quot;))[22])" />
<user NXS:location="(GET(&quot;USER&quot;))[1]" />
<inst_short NXS:location="SUBSTRING(GET(&quot;HDR&quot;),1,3)" />
<inst_full NXS:location="GET(&quot;NAME&quot;)" />
<start_date NXS:location="SUBSTRING(GET(&quot;HDR&quot;),53,64)" />
<start_time NXS:location="SUBSTRING(GET(&quot;HDR&quot;),65,72)" />
<finish_date NXS:location="(GET(&quot;CRPB&quot;))[17]" />
<finish_time NXS:location="(GET(&quot;CRPB&quot;))[20]" />
<start_time NXS:location="SUBSTRING(GET(&quot;HDR&quot;),65,72)" />
<user NXS:location="(GET(&quot;USER&quot;))[1]" />
<duration_sec NXS:location="(GET(&quot;IRPB&quot;))[13]" /> <!-- or [1] -->
<good_uamph NXS:location="(GET(&quot;RRPB&quot;))[8]" />
<raw_uamph NXS:location="(GET(&quot;RRPB&quot;))[9]" />
<twotheta NXS:location="GET(&quot;TTHE&quot;)" />
</entry1>
</NXroot>
